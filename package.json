{
  "name": "board-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jquery": "^3.5.0",
    "prettier": "^2.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fix": "prettier --single-quote --tab-width 2  --trailing-comma es5 --write \"src/**/*.{ts,js,json,scss,css,jsx,tsx,html}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
